{
    "name": "terpdev/cubewikipackage",
    "description": "A Filament plugin to display WikiCube knowledge base content via API tokens",
    "keywords": [
        "TerpDev",
        "laravel",
        "cubewikipackage",
        "filament",
        "knowledge-base"
    ],
    "homepage": "https://github.com/terpdev/cubewikipackage",
    "support": {
        "issues": "https://github.com/terpdev/cubewikipackage/issues",
        "source": "https://github.com/terpdev/cubewikipackage"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "terpdev",
            "email": "danielterpstra006@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "filament/filament": "^3.0|^4.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9|^8.0",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.1|^3.0",
        "pestphp/pest-plugin-arch": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "TerpDev\\CubeWikiPackage\\": "src/",
            "TerpDev\\CubeWikiPackage\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TerpDev\\CubeWikiPackage\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TerpDev\\CubeWikiPackage\\CubeWikiPackageServiceProvider"
            ],
            "aliases": {
                "CubeWikiPackage": "TerpDev\\CubeWikiPackage\\Facades\\CubeWikiPackage"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

